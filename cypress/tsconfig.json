{
	"compilerOptions": {
		"target": "es5",
		"lib": ["es5", "dom"],
		"types": ["cypress"],
		"paths": {
			"@pages/*": ["../src/pages/*"],
			"@atoms/*": ["../src/atoms/*"],
			"@molecules/*": ["../src/molecules/*"],
			"@organisms/*": ["../src/organisms/*"],
			"@layouts/*": ["../src/layouts/*"],
			"@hooks/*": ["../src/hooks/*"],
			"@utils/*": ["../src/utils/*"],
			"@schema/*": ["../src/schema/*"],
			"@styles/*": ["../src/styles/*"],
			"@context/*": ["../src/context/*"]
		}
	},
	"include": ["**/*.js", "**/*.ts"]
}
