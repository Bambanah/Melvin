{
	"name": "melvin",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"build:1-migrate": "prisma generate",
		"build:2-build": "next build",
		"build": "run-s build:*",
		"build-ci": "run-s build:* prisma:push",
		"db:nuke": "docker-compose down --volumes --remove-orphans",
		"db:reset": "run-s db:nuke db:up prisma:dev prisma:seed",
		"db:up": "docker-compose up -d",
		"db:down": "docker-compose down",
		"prisma:generate": "prisma generate",
		"prisma:seed": "prisma db seed",
		"prisma:dev": "prisma migrate dev --skip-seed",
		"prisma:push": "prisma db push",
		"dev": "next dev",
		"dx": "run-s db:up dev",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"lint:ts": "tsc --pretty --noEmit",
		"test:unit": "jest",
		"test:e2e": "playwright test",
		"test": "run-s test:*",
		"pw:codegen": "playwright codegen http://localhost:3000 --load-storage=e2e/setup/storage-state.json",
		"ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
		"postinstall": "prisma generate"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"prisma": {
		"seed": "yarn ts-node prisma/seed.ts"
	},
